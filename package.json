{
  "name": "react-asteroids",
  "version": "1.0.0",
  "description": "Simple game using React/Redux and react-konva for defining canvas objecs in JSX",
  "main": "index.js",
  "scripts": {
    "start": "node dist",
    "build": "webpack --config webpack.config.js --progress",
    "watch": "webpack --config webpack.config.js --progress --watch",
    "test": "karma start",
    "tdd": "karma start --auto-watch --no-single-run",
    "build-server": "babel server -d dist",
    "watch-server": "babel server -d dist --watch --source-maps",
    "deploy": "NODE_ENV=prod webpack -p"
